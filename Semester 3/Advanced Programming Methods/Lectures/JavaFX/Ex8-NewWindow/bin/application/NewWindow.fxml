<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>


<StackPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="application.NewWindowController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button fx:id="button1" mnemonicParsing="false" onAction="#onButton1" text="Open window 1" />
            <Button fx:id="button2" mnemonicParsing="false" onAction="#onButton2" text="Open window 2" />
         </children>
      </VBox>
   </children>
</StackPane>
