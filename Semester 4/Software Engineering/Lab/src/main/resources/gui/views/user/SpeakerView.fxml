<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
          prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <rowConstraints>
        <RowConstraints maxHeight="342.0" minHeight="10.0" prefHeight="52.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="342.0" minHeight="10.0" prefHeight="52.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="342.0" minHeight="10.0" prefHeight="52.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="180.0"/>
    </columnConstraints>
    <Label text="You have papers for the following sessions:" GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.columnIndex="0"
           GridPane.columnSpan="2"/>

    <ListView fx:id="sessionsListView" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="200.0"
              prefWidth="200.0" GridPane.halignment="CENTER" GridPane.rowIndex="1"
              GridPane.columnIndex="0" GridPane.columnSpan="2"/>

    <Button fx:id="goBackButton" mnemonicParsing="false" text="Go Back" GridPane.halignment="CENTER"
            GridPane.columnIndex="0" GridPane.rowIndex="2" prefWidth="150.0"/>
    <Button fx:id="goToSessionButton" mnemonicParsing="false" text="Go To Session" GridPane.halignment="CENTER"
            GridPane.columnIndex="1" GridPane.rowIndex="2" prefWidth="150.0"/>
</GridPane>
