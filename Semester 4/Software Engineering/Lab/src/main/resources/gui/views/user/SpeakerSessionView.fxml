<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
          prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <rowConstraints>
        <RowConstraints maxHeight="342.0" minHeight="10.0" prefHeight="52.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="342.0" minHeight="10.0" prefHeight="52.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="342.0" minHeight="10.0" prefHeight="52.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="342.0" minHeight="10.0" prefHeight="52.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="180.0"/>
    </columnConstraints>

    <Label fx:id="paperTextLabel" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.halignment="CENTER" />
    <Label text="You have to present this paper at the time: " GridPane.columnIndex="0" GridPane.rowIndex="1" GridPane.halignment="CENTER" />
    <Label fx:id="timeToPresentLabel" GridPane.columnIndex="0" GridPane.rowIndex="2" GridPane.halignment="CENTER" />
    <VBox GridPane.halignment="CENTER"
          GridPane.columnIndex="0" GridPane.rowIndex="3" prefWidth="150.0">
        <Button fx:id="addPresentationButton" mnemonicParsing="false" text="Upload Presentation" prefWidth="150.0"/>
        <Button fx:id="back" mnemonicParsing="false" text="Back" prefWidth="150.0"/>
    </VBox>
</GridPane>
