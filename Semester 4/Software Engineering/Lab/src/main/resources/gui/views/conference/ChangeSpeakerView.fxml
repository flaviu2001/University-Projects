<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          prefHeight="600.0" prefWidth="800.0">
    <columnConstraints>
        <ColumnConstraints maxWidth="400.0" minWidth="400.0" prefWidth="400.0" />
        <ColumnConstraints maxWidth="400.0" minWidth="400.0" prefWidth="400.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="150.0" minHeight="150.0" prefHeight="150.0" />
        <RowConstraints maxHeight="150.0" minHeight="150.0" prefHeight="150.0" />
        <RowConstraints maxHeight="150.0" minHeight="150.0" prefHeight="150.0" />
        <RowConstraints maxHeight="150.0" minHeight="150.0" prefHeight="150.0" />
    </rowConstraints>
    <ListView fx:id="users" GridPane.rowIndex="0" GridPane.rowSpan="4" GridPane.columnIndex="0"/>
    <Button fx:id="add" GridPane.rowIndex="0" GridPane.columnIndex="1" text="Add" GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
    <Button fx:id="delete" GridPane.rowIndex="1" GridPane.columnIndex="1" text="Delete" GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
    <Button fx:id="pay" text="Pay" GridPane.rowIndex="2" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
    <Button fx:id="back" text="Back" GridPane.rowIndex="3" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
</GridPane>
